			 	<param name="projectId">${project.id}</param>
			 </result>
			 <result name="input" type="tiles">submissionIc</result>
			 <param name="projectId">${project.id}</param>
		</action>
		<action name="listIc" method="getIcList" class="gov.nih.nci.cbiit.scimgmt.gds.actions.IcSubmissionAction">
			 <result name="success" type="tiles">submissionIcList</result>
			 <result name="input" type="tiles">submissionIcList</result>
			  <result name="empty" type="redirect-action">
			 	<param name="actionName">editIc</param>
			 	<param name="projectId">${project.id}</param>
			 </result>
			 <param name="projectId">${project.id}</param>
		</action>
