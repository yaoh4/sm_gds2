<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	
	<package name="manage" namespace="/manage" extends="default">
	
		<interceptors>
			<interceptor-stack name="projectScopeStack">
				<interceptor-ref name="logger" />
				<interceptor-ref name="scope" >
		    		<param name="key">stored</param>
		    		<param name="session">project</param>
		    	</interceptor-ref>					
				<interceptor-ref name="defaultStack" />	
				<interceptor-ref name="userInterceptor" />					
			</interceptor-stack>	
		</interceptors>
		
		<default-interceptor-ref name="projectScopeStack" />
	
		<action name="home"	class="gov.nih.nci.cbiit.scimgmt.gds.actions.SearchProjectAction">
			 <result name="success" type="tiles">home</result>
		</action>
		<action name="newSubmission" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionGeneralInfo</result>
		</action>
		<action name="saveGeneralInfo" method="save" class="gov.nih.nci.cbiit.scimgmt.gds.actions.GeneralInfoSubmissionAction">
			 <result name="success" type="tiles">submissionGeneralInfo</result>
		</action>
		<action name="saveGeneralInfoAndNext" method="saveAndNext" class="gov.nih.nci.cbiit.scimgmt.gds.actions.GeneralInfoSubmissionAction">
			 <result name="success" type="redirect-action">editGdsPlan</result>
		</action>
		<action name="editGdsPlan" method="execute" class="gov.nih.nci.cbiit.scimgmt.gds.actions.GDSPlanSubmissionAction">
			 <result name="success" type="tiles">submissionGdsPlan</result>
		</action>
		<action name="saveGdsPlan" method="save" class="gov.nih.nci.cbiit.scimgmt.gds.actions.GDSPlanSubmissionAction">
			 <result name="success" type="tiles">submissionIcList</result>
		</action>
		<action name="editIc" class="gov.nih.nci.cbiit.scimgmt.gds.actions.IcSubmissionAction">
			 <result name="success" type="tiles">submissionIc</result>
		</action>
		<action name="deleteIcAndDul" method="deleteIcAndDul" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionIcList</result>
		</action>
		<action name="saveIcAndDul" method="saveIcAndDul" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionIcList</result>
		</action>
		<action name="addStudy" method="addStudy" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionIcAndDul</result>
		</action>
		<action name="deleteStudy" method="deleteStudy" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionIcAndDul</result>
		</action>
		<action name="addDul" method="addDul" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionIcAndDul</result>
		</action>
		<action name="deleteDul" method="deleteDul" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionIcAndDul</result>
		</action>
		<action name="navigateToBasicStudyInfo" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionBasicStudyInfo</result>
		</action>
		<action name="saveBasicStudyInfo" method="saveBasicStudyInfo" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionDetail</result>
		</action>
		<action name="downloadFile" method="downloadFile" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionGdsPlan</result>
		</action>
		<action name="deleteFile" method="deleteFile" class="gov.nih.nci.cbiit.scimgmt.gds.actions.ManageSubmission">
			 <result name="success" type="tiles">submissionGdsPlan</result>
		</action>
		<action name="navigateToRepositoryStatus" class="gov.nih.nci.cbiit.scimgmt.gds.actions.RepositoryStatusSubmissionAction">
			 <result name="success" type="tiles">submissionRepositoryStatus</result>
		</action>		
		<action name="navigateToIntramuralGrantOrContractSearch" class="gov.nih.nci.cbiit.scimgmt.gds.actions.RepositoryStatusSubmissionAction">
			 <result name="success" type="tiles">intramuralGrantOrContractSearch</result>
		</action>
	</package>
</struts>
