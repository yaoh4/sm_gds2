			<result name="success" type="dispatcher">/jsp/content/submissionGdsPlanFile.jsp</result>
			<result name="input" type="stream">     
				<param name="contentType">text/html</param>     
				<param name="inputName">inputStream</param>   
			</result>
		</action>
